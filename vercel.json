{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [{
            "src": "^/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "^/about.html",
            "dest": "/about.html"
        },
        {
            "src": "^/contact.html",
            "dest": "/contact.html"
        },
        {
            "src": "^/Praneeth_Resume.pdf",
            "dest": "/Praneeth_Resume.pdf"
        },
        {
            "src": "^/robots.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "^/(.*)",
            "dest": "/index.html"
        }
    ],
    "headers": [{
        "source": "/(.*)",
        "headers": [{
                "key": "X-Content-Type-Options",
                "value": "nosniff"
            },
            {
                "key": "X-Frame-Options",
                "value": "SAMEORIGIN"
            },
            {
                "key": "X-XSS-Protection",
                "value": "1; mode=block"
            },
            {
                "key": "cache-control",
                "value": "public, max-age=60, s-maxage=86400, stale-while-revalidate=86400, stale-if-error=86400"
            },
            {
                "key": "Feature-Policy",
                "value": "camera 'none'; microphone 'none'"
            },
            {
                "key": "Referrer-Policy",
                "value": "strict-origin"
            },
            {
                "key": "Content-Security-Policy",
                "value": "default-src 'self' https://fonts.gstatic.com https://kit-free.fontawesome.com data:; script-src 'self' https://stackpath.bootstrapcdn.com https://browser.sentry-cdn.com https://kit.fontawesome.com https://cdnjs.cloudflare.com https://code.jquery.com 'unsafe-inline'; connect-src 'self' https://o413953.ingest.sentry.io; img-src 'self'; style-src 'self' https://stackpath.bootstrapcdn.com https://fonts.googleapis.com https://fonts.gstatic.com https://kit-free.fontawesome.com;base-uri 'self'"
            }
        ]
    }]
}